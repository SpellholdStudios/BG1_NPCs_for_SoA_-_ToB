BEGIN @1903 DESIGNATED 4
GROUP ~Main Mod~
REQUIRE_PREDICATE GAME_IS ~bg2 tob bgt bg2ee eet~ ~Not support game~

REQUIRE_PREDICATE (NOT FILE_EXISTS_IN_GAME ~lk#yesl.cre~) AND
